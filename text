model User {
  id          String       @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  email       String       @unique
  password    String
  bio         String
  title       String
  location    String
  image       String?
  isAvailable Boolean
  skills      Skill[]
  experiences Experience[]
  educations  Education[]

  @@map("users")
}